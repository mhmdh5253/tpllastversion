    <script>
        document.addEventListener('DOMContentLoaded', () => {
                const initPlyr = (player) => {
                        const instance = new Plyr(player, {
                                ratio: '16:9',
                                hideControls: false
                            });

                        instance.on('ready', () => {
                                if(instance.isHTML5) {
                                        instance.resize();
                                        setTimeout(() => instance.resize(), 500);
                                    }
                            });

                        return instance;
                    };

                document.querySelectorAll('.plyr-video-player').forEach(initPlyr);
            });
    </script>
